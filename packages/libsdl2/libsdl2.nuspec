<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata minClientVersion="2.12">
    <id>libsdl2</id>
    <version>$version$</version>
    <authors>Ronald van Manen</authors>
    <owners>Ronald van Manen</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">Zlib</license>
    <projectUrl>https://github.com/ronaldvanmanen/SDL2Sharp</projectUrl>
    <description>Multi-platform native library for SDL2.</description>
    <copyright>Copyright Â© Ronald van Manen</copyright>
    <repository type="git" url="https://github.com/ronaldvanmanen/SDL2Sharp" branch="main" />
    <dependencies>
      <group targetFramework="net5.0" />
      <group targetFramework="native0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="runtime.json" target="runtime.json" />
    <file src="lib\net5.0\_._" target="lib\net5.0\_._" />
    <file src="include\**\*.h" target="lib\native\include" />
  </files>
</package>
